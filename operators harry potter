#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int number;
    int f,l,sum;
   
    scanf("%d",&number);
     if(number<0)
    {
        number=-number;
    }
    f=number%10;
    l=number/1000;
    sum=f+l;
    printf("%d",sum);
    
        
    return 0;
}
